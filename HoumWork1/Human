package Tree;

public class Human {
    private String gender;
    private static String name;
    private String mother;
    private String father;

    Human(String gender, String name, String mother, String father) {
        this.gender = gender;
        this.name = name;
        this.mother = mother;
        this.father = father;
    }
    public String getGender() {
        return gender;
    }
    public static String getName() {
        return name;
    }
    public String getMother() {
        return mother;
    }
    public String getFather() {
        return father;
    }


    @Override
    public String toString() {
        //return "Пол:  " + gender + " " + "Имя:  " + name + " " + "Мать:  " + mother + " " + "Отец:  " + father;
        return "Пол:  " + gender + " " + "Имя:  " + name;
    }
}
