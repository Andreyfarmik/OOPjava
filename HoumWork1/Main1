package Tree;

public class Main1 {
    public static void main(String[] args){
        Human human1 = new Human("Мужской","Валерий", "Нина","Иван");
        Human human2 = new Human("Женский","Марина", "Татьяна","Игорь");
        Human human3 = new Human("Мужской","Виталий", "Ирина","Петр");
        Human human4 = new Human("Мужской","Николай", "Александра","Олег");

        FamilyTree familyTree = new FamilyTree();
        familyTree.addHuman(human1);
        familyTree.addHuman(human2);
        familyTree.addHuman(human3);
        familyTree.addHuman(human4);
        System.out.println(familyTree.getInfo());
        System.out.println(familyTree.scrHuman("Валерий"));
    }
}
